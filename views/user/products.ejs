<%-include("../user/layouts/user-header.ejs")%>
<% products.forEach((product) => { %>
  
  <div className="card" style="width: 18rem;">
    <img src="images/products/<%= product.image[0] %>" className="card-img-top" alt="Your Alt Text" />
    <div className="card-body">
      <h5 className="card-title"><%= product.name %></h5>
      <p className="card-text"><%= product.price %></p>
      <a href="#">
        <button type="button" class="btn btn-primary">Add to Cart</button>
      </a>
      <a href="http://localhost:3000/admin/add-product">
        <button type="button" class="btn btn-primary">Admin</button>
      </a>
    </div>
  </div>
  <% }) %>
<%-include("../user/layouts/user-footer.ejs")%>